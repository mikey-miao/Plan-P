{
  "manifest_version": 3,
  "name": "Plan P Project Sorter",
  "version": "1.0.0",
  "description": "静态项目清单工具的扩展弹窗页面。",
  "icons": {
    "16": "icons.png",
    "48": "icons.png",
    "128": "icons.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "Project Sorter",
    "default_icon": {
      "16": "icons.png",
      "48": "icons.png",
      "128": "icons.png"
    }
  },
  "permissions": [
    "storage"
  ]
}


